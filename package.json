{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@auth0/auth0-react": "^1.4.0",
    "@auth0/nextjs-auth0": "^1.6.1",
    "@cobuildlab/react-simple-state": "^0.6.2",
    "@graphql-codegen/cli": "^1.21.6",
    "@graphql-codegen/typed-document-node": "^1.18.9",
    "@graphql-codegen/typescript": "^1.22.4",
    "@graphql-codegen/typescript-operations": "^1.18.3",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/pickers": "^3.3.10",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/react-dom": "^17.0.0",
    "apollo-client": "^2.6.10",
    "apollo-upload-client": "^16.0.0",
    "axios": "^0.24.0",
    "babel-plugin-styled-components": "^1.13.2",
    "clsx": "^1.1.1",
    "d": "^1.0.1",
    "dotenv": "^10.0.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "express": "^4.17.2",
    "graphql": "^15.5.0",
    "highcharts": "^9.3.1",
    "highcharts-react-official": "^3.1.0",
    "jest": "^26.6.0",
    "moment": "^2.29.1",
    "next": "^10.1.3",
    "nprogress": "^0.2.0",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.17.4",
    "react-material-ui-carousel": "^2.3.5",
    "react-phone-number-input": "^3.1.38",
    "react-scripts": "4.0.3",
    "serve": "^13.0.2",
    "styled-components": "^5.3.3",
    "validator": "^13.7.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "schema-generate": "graphql-codegen --config codegen.yml",
    "schema-generate:local": "graphql-codegen --config codegen.local.yml"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged",
      "pre-push:": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,ts, tsx}": [
      "prettier --write",
      "eslint --fix",
      "npm run build"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testRegex": [
      ".test.ts$",
      ".test.tsx$"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@types/node": "^12.20.10",
    "@types/react": "^17.0.3",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-react": "^7.23.2",
    "helmet": "^5.0.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "typescript": "^4.2.4"
  }
}
